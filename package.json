{
  "name": "@mojir/lits",
  "version": "2.1.39",
  "description": "Lits is a pure functional programming language implemented in TypeScript",
  "author": "Albert Mojir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojir/lits.git"
  },
  "keywords": [
    "Lits",
    "Functional",
    "Pure",
    "Typescript",
    "Web",
    "Compiler",
    "AST",
    "Interpreter",
    "Clojure"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./full": {
      "types": "./dist/src/full.d.ts",
      "import": "./dist/full.esm.js",
      "require": "./dist/full.js"
    },
    "./namespaces/assert": {
      "types": "./dist/src/namespaces/assert.d.ts",
      "import": "./dist/namespaces/assert.esm.js",
      "require": "./dist/namespaces/assert.js"
    },
    "./namespaces/grid": {
      "types": "./dist/src/namespaces/grid.d.ts",
      "import": "./dist/namespaces/grid.esm.js",
      "require": "./dist/namespaces/grid.js"
    },
    "./namespaces/random": {
      "types": "./dist/src/namespaces/random.d.ts",
      "import": "./dist/namespaces/random.esm.js",
      "require": "./dist/namespaces/random.js"
    },
    "./namespaces/vector": {
      "types": "./dist/src/namespaces/vector.d.ts",
      "import": "./dist/namespaces/vector.esm.js",
      "require": "./dist/namespaces/vector.js"
    },
    "./namespaces/linearAlgebra": {
      "types": "./dist/src/namespaces/linearAlgebra.d.ts",
      "import": "./dist/namespaces/linearAlgebra.esm.js",
      "require": "./dist/namespaces/linearAlgebra.js"
    },
    "./namespaces/matrix": {
      "types": "./dist/src/namespaces/matrix.d.ts",
      "import": "./dist/namespaces/matrix.esm.js",
      "require": "./dist/namespaces/matrix.js"
    },
    "./namespaces/numberTheory": {
      "types": "./dist/src/namespaces/numberTheory.d.ts",
      "import": "./dist/namespaces/numberTheory.esm.js",
      "require": "./dist/namespaces/numberTheory.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "iife": "dist/lits.iife.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "lits": "dist/cli/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "vitest": "vitest",
    "lint": "eslint . --fix",
    "lint:no-fix": "eslint .",
    "typecheck": "tsc -p ./tsconfig.compile.json --noEmit",
    "check": "npm run lint && npm run typecheck && npm run test && npm run build",
    "check:no-fix": "npm run lint:no-fix && npm run typecheck && npm run test",
    "clean": "npm run clean-lits && npm run clean-playground",
    "clean-lits": "rm -rf dist build",
    "clean-playground": "rm -rf playground-builder/build playground-www/build",
    "build": "npm run build-lits && npm run build-cli && npm run build-playground",
    "build-lits": "npm run clean-lits && rollup -c",
    "build-playground": "npm run clean-playground && rollup -c rollup.config.playground-builder.js && rollup -c rollup.config.playground-www.js && node ./playground-builder/build/buildPlaygroundSite.js",
    "build-cli": "rollup -c rollup.config.cli.js",
    "lits": "node ./dist/cli/cli.js",
    "dev": "npx serve docs -p 9901",
    "lcov": "open-cli ./coverage/index.html"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "22.7.6",
    "@vitest/coverage-v8": "1.6.0",
    "open-cli": "8.0.0",
    "rollup": "4.18.0",
    "serve": "^14.2.4",
    "tslib": "2.6.3",
    "typescript": "5.5.2",
    "vitest": "1.6.0"
  }
}
